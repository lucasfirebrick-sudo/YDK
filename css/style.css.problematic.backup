/* ========== 产品标签统一样式（覆盖所有之前定义）========== */
.product-label {
    position: absolute !important;
    top: 8px !important;
    right: 8px !important;
    z-index: 10 !important;
    padding: 4px 8px !important;
    border-radius: 12px !important;
    font-size: 11px !important;
    font-weight: var(--font-weight-semibold) !important;
