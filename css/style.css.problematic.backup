    letter-spacing: 0.5px;
}

.project-overlay p {
    font-size: var(--font-size-base);
    font-weight: var(--font-weight-semibold);
    opacity: 1;
    line-height: 1.4;
    color: var(--bg-white);
    text-shadow:
        0 1px 6px rgba(0, 0, 0, 0.8),
        0 0 15px rgba(0, 0, 0, 0.5);
