    bottom: -10px;
    left: 50%;
    transform: translateX(-50%);
    width: 60px;
    height: 3px;
    background: linear-gradient(135deg, #ff9800, #f57c00);
    border-radius: 2px;
}

.industry-grid-optimized {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 1.5rem;
