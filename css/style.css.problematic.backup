    padding: 40px 0;
    background: linear-gradient(135deg, var(--bg-light) 0%, var(--bg-white) 100%);
    position: relative;
    overflow: hidden;
}

.faq-section-enhanced::before {
    content: '';
    position: absolute;
    top: -50%;
    right: -20%;
    width: 100%;
    height: 200%;
    background: radial-gradient(circle, rgba(255, 107, 107, 0.05) 0%, transparent 70%);
    transform: rotate(30deg);
    z-index: 1;
}

.faq-section-enhanced .container {
    position: relative;
    z-index: 2;
}

/* FAQ Header */
.faq-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 60px;
    gap: 40px;
}

.faq-header .header-content {
    flex: 1;
}

.faq-header .section-badge {
    display: inline-block;
    background: linear-gradient(135deg, #ff6b6b, #ff8e8e);
    color: var(--bg-white);
    padding: 8px 20px;
    border-radius: 25px;
    font-size: 14px;
    font-weight: var(--font-weight-semibold);
    margin-bottom: 15px;
    box-shadow: 0 4px 15px rgba(255, 107, 107, 0.3);
}

.faq-main-title {
    font-size: 2.8rem;
    font-weight: var(--font-weight-bold);
    color: #2c3e50;
    margin-bottom: 15px;
    line-height: 1.2;
}

.faq-subtitle {
    font-size: 1.2rem;
    color: #7f8c8d;
    margin: 0;
    line-height: 1.6;
}

.faq-stats {
    display: flex;
    gap: 30px;
    align-items: center;
}

.faq-stats .stat-item {
    text-align: center;
    padding: 20px;
    background: var(--bg-white);
    border-radius: 15px;
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.08);
    border: 1px solid #f1f2f6;
    min-width: 120px;
}

.faq-stats .stat-number {
    display: block;
    font-size: 2rem;
    font-weight: var(--font-weight-bold);
    color: #ff6b6b;
    margin-bottom: 5px;
}

.faq-stats .stat-text {
    font-size: 0.9rem;
    color: #7f8c8d;
    font-weight: var(--font-weight-medium);
}

/* FAQ Categories */
.faq-categories {
    display: flex;
    justify-content: center;
    gap: 15px;
    margin-bottom: 40px;
    flex-wrap: wrap;
}

.category-btn {
    display: flex;
    align-items: center;
    gap: 8px;
    padding: 12px 24px;
    background: var(--bg-white);
    border: 2px solid var(--border-color);
    border-radius: 25px;
    font-size: 14px;
    font-weight: var(--font-weight-semibold);
    color: #6c757d;
    cursor: pointer;
    transition: all 0.3s ease;
    text-decoration: none;
}

.category-btn:hover {
    border-color: #ff6b6b;
    color: #ff6b6b;
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(255, 107, 107, 0.2);
}

.category-btn.active {
    background: linear-gradient(135deg, #ff6b6b, #ff8e8e);
    border-color: #ff6b6b;
    color: var(--bg-white);
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(255, 107, 107, 0.3);
}

.category-btn i {
    font-size: 16px;
}

/* FAQ Search */
.faq-search {
    margin-bottom: 50px;
    display: flex;
    justify-content: center;
}

.search-box {
    position: relative;
    max-width: 500px;
    width: 100%;
}

.search-box i {
    position: absolute;
    left: 20px;
    top: 50%;
    transform: translateY(-50%);
    color: #adb5bd;
    font-size: 18px;
}

.search-box input {
    width: 100%;
    padding: 18px 60px 18px 55px;
    border: 2px solid var(--border-color);
    border-radius: 30px;
    font-size: 16px;
    background: var(--bg-white);
    transition: all 0.3s ease;
    outline: none;
}

.search-box input:focus {
    border-color: #ff6b6b;
    box-shadow: 0 0 0 3px rgba(255, 107, 107, 0.1);
}

.search-clear {
    position: absolute;
    right: 15px;
    top: 50%;
    transform: translateY(-50%);
    background: none;
    border: none;
    color: #adb5bd;
    cursor: pointer;
    padding: 5px;
    display: none;
    transition: color 0.3s ease;
}

.search-clear:hover {
    color: #ff6b6b;
}

/* Popular Questions */
.popular-questions {
    margin-bottom: 50px;
}

.popular-questions .section-heading {
    display: flex;
    align-items: center;
    gap: 10px;
    font-size: var(--font-size-2xl);
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin-bottom: 25px;
}

.popular-questions .section-heading i {
    color: #ff6b6b;
    font-size: 1.3rem;
}

.quick-answers {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 20px;
}

.quick-answer-item {
    display: flex;
    align-items: center;
    gap: 15px;
    padding: 20px;
    background: var(--bg-white);
    border: 2px solid #f1f2f6;
    border-radius: 15px;
    cursor: pointer;
    transition: all 0.3s ease;
}

.quick-answer-item:hover {
    border-color: #ff6b6b;
    transform: translateY(-3px);
    box-shadow: 0 10px 30px rgba(255, 107, 107, 0.15);
}

.quick-icon {
    width: 50px;
    height: 50px;
    background: linear-gradient(135deg, #ff6b6b, #ff8e8e);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--bg-white);
    font-size: 20px;
    flex-shrink: 0;
}

.quick-answer-item span {
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    font-size: 15px;
}

/* FAQ List */
.faq-list {
    display: flex;
    flex-direction: column;
    gap: 20px;
}

.faq-item-enhanced {
    background: var(--bg-white);
    border: 2px solid #f1f2f6;
    border-radius: 20px;
    overflow: hidden;
    transition: all 0.3s ease;
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.05);
}

.faq-item-enhanced:hover {
    border-color: #ff6b6b;
    box-shadow: 0 10px 30px rgba(255, 107, 107, 0.1);
}

.faq-item-enhanced.active {
    border-color: #ff6b6b;
    box-shadow: 0 10px 30px rgba(255, 107, 107, 0.15);
}

/* FAQ Questions */
.faq-question-enhanced {
    padding: 25px 30px;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 20px;
    transition: all 0.3s ease;
}

.faq-question-enhanced:hover {
    background: var(--bg-light);
}

.question-content {
    display: flex;
    align-items: center;
    gap: 20px;
    flex: 1;
}

.question-icon {
    width: 50px;
    height: 50px;
    background: linear-gradient(135deg, #ff6b6b, #ff8e8e);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--bg-white);
    font-size: 20px;
    flex-shrink: 0;
}

.question-text h3 {
    font-size: 1.3rem;
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin: 0 0 10px 0;
    line-height: 1.4;
}

.question-tags {
    display: flex;
    gap: 8px;
    flex-wrap: wrap;
}

.question-tags .tag {
    background: #e3f2fd;
    color: #1976d2;
    padding: 4px 12px;
    border-radius: 15px;
    font-size: 12px;
    font-weight: var(--font-weight-medium);
}

.toggle-icon {
    width: 40px;
    height: 40px;
    background: var(--bg-light);
    border: 2px solid var(--border-color);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease;
    flex-shrink: 0;
}

.faq-item-enhanced.active .toggle-icon {
    background: #ff6b6b;
    border-color: #ff6b6b;
    color: var(--bg-white);
}

.toggle-icon i {
    font-size: 16px;
    transition: all 0.3s ease;
}

/* FAQ Answers */
.faq-answer-enhanced {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.4s ease-out, padding 0.4s ease-out;
    background: #fafbfc;
    border-top: 1px solid #f1f2f6;
}

.faq-item-enhanced.active .faq-answer-enhanced {
    padding: 30px;
}

.answer-content {
    line-height: 1.7;
}

.answer-overview {
    margin-bottom: 25px;
    padding: 20px;
    background: var(--bg-white);
    border-radius: 15px;
    border-left: 4px solid #ff6b6b;
}

.answer-overview p {
    margin: 0;
    font-size: 16px;
    color: #2c3e50;
    font-weight: var(--font-weight-medium);
}

/* Detail Grid */
.detail-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 20px;
    margin-top: 20px;
}

.detail-item {
    display: flex;
    align-items: flex-start;
    gap: 15px;
    padding: 20px;
    background: var(--bg-white);
    border-radius: 12px;
    border: 1px solid #f1f2f6;
}

.detail-item i {
    width: 40px;
    height: 40px;
    background: linear-gradient(135deg, #ff6b6b, #ff8e8e);
    border-radius: 10px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--bg-white);
    font-size: 18px;
    flex-shrink: 0;
}

.detail-item h4 {
    font-size: 16px;
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin: 0 0 5px 0;
}

.detail-item p {
    font-size: 14px;
    color: #7f8c8d;
    margin: 0;
    line-height: 1.5;
}

/* Payment Methods */
.payment-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 20px;
}

.payment-item {
    padding: 25px 20px;
    background: var(--bg-white);
    border-radius: 15px;
    border: 2px solid #f1f2f6;
    text-align: center;
    transition: all 0.3s ease;
}

.payment-item:hover {
    border-color: #ff6b6b;
    transform: translateY(-3px);
    box-shadow: 0 10px 25px rgba(255, 107, 107, 0.15);
}

.payment-icon {
    width: 60px;
    height: 60px;
    background: linear-gradient(135deg, #ff6b6b, #ff8e8e);
    border-radius: 15px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--bg-white);
    font-size: 24px;
    margin: 0 auto 15px;
}

.payment-info h4 {
    font-size: 16px;
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin: 0 0 5px 0;
}

.payment-info p {
    font-size: 14px;
    color: #7f8c8d;
    margin: 0 0 10px 0;
}

.payment-note {
    display: inline-block;
    background: #e8f5e8;
    color: #2e7d32;
    padding: 4px 10px;
    border-radius: 12px;
    font-size: 12px;
    font-weight: var(--font-weight-medium);
}

/* Certifications Showcase */
.cert-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 20px;
}

.cert-item {
    display: flex;
    align-items: center;
    gap: 15px;
    padding: 20px;
    background: var(--bg-white);
    border-radius: 12px;
    border: 2px solid #f1f2f6;
    transition: all 0.3s ease;
}

.cert-item:hover {
    border-color: #ff6b6b;
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(255, 107, 107, 0.15);
}



.cert-info h4 {
    font-size: 15px;
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin: 0 0 5px 0;
}

.cert-info p {
    font-size: 13px;
    color: #7f8c8d;
    margin: 0;
}

/* Service Features */
.service-overview {
    margin-bottom: 25px;
    padding: 20px;
    background: var(--bg-white);
    border-radius: 15px;
    border-left: 4px solid #ff6b6b;
}

.service-overview p {
    margin: 0;
    font-size: 16px;
    color: #2c3e50;
    line-height: 1.6;
}

.feature-row {
    display: flex;
    flex-wrap: wrap;
    gap: 15px;
}

.feature-item {
    display: flex;
    align-items: center;
    gap: 10px;
    padding: 12px 18px;
    background: var(--bg-white);
    border: 2px solid #f1f2f6;
    border-radius: 25px;
    font-size: 14px;
    font-weight: var(--font-weight-medium);
    color: #2c3e50;
}

.feature-item i {
    color: #ff6b6b;
    font-size: 16px;
}

/* Support Channels */
.channel-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 20px;
}

.channel-item {
    padding: 25px 20px;
    background: var(--bg-white);
    border-radius: 15px;
    border: 2px solid #f1f2f6;
    text-align: center;
    transition: all 0.3s ease;
}

.channel-item:hover {
    border-color: #ff6b6b;
    transform: translateY(-3px);
    box-shadow: 0 10px 25px rgba(255, 107, 107, 0.15);
}

.channel-icon {
    width: 60px;
    height: 60px;
    background: linear-gradient(135deg, #2196f3, #42a5f5);
    border-radius: 15px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--bg-white);
    font-size: 24px;
    margin: 0 auto 15px;
}

.channel-info h4 {
    font-size: 16px;
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin: 0 0 5px 0;
}

.channel-info p {
    font-size: 14px;
    color: #7f8c8d;
    margin: 0 0 10px 0;
}

.availability {
    display: inline-block;
    background: #e8f5e8;
    color: #2e7d32;
    padding: 4px 10px;
    border-radius: 12px;
    font-size: 12px;
    font-weight: var(--font-weight-medium);
}

/* Warranty Info */
.warranty-main {
    display: flex;
    align-items: center;
    gap: 30px;
    padding: 25px;
    background: var(--bg-white);
    border-radius: 15px;
    border: 2px solid #f1f2f6;
}

.warranty-period {
    text-align: center;
    padding: 20px;
    background: linear-gradient(135deg, #ff6b6b, #ff8e8e);
    border-radius: 15px;
    color: var(--bg-white);
    min-width: 150px;
}

.period-number {
    font-size: 3rem;
    font-weight: var(--font-weight-bold);
    display: block;
    line-height: 1;
}

.period-unit {
    font-size: 1.2rem;
    font-weight: var(--font-weight-medium);
    display: block;
}

.period-text {
    font-size: 14px;
    opacity: 0.9;
    display: block;
    margin-top: 5px;
}

.warranty-features {
    flex: 1;
}

.warranty-feature {
    display: flex;
    align-items: center;
    gap: 12px;
    margin-bottom: 12px;
    font-size: 15px;
    color: #2c3e50;
}

.warranty-feature:last-child {
    margin-bottom: 0;
}

.warranty-feature i {
    color: #4caf50;
    font-size: 18px;
}

/* Sample Services */
.sample-types {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 20px;
}

.sample-type {
    display: flex;
    align-items: flex-start;
    gap: 15px;
    padding: 25px;
    background: var(--bg-white);
    border-radius: 15px;
    border: 2px solid #f1f2f6;
    transition: all 0.3s ease;
}

.sample-type:hover {
    border-color: #ff6b6b;
    transform: translateY(-3px);
    box-shadow: 0 10px 25px rgba(255, 107, 107, 0.15);
}

.sample-icon {
    width: 50px;
    height: 50px;
    background: linear-gradient(135deg, #9c27b0, #ba68c8);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--bg-white);
    font-size: 20px;
    flex-shrink: 0;
}

.sample-info h4 {
    font-size: 16px;
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin: 0 0 5px 0;
}

.sample-info p {
    font-size: 14px;
    color: #7f8c8d;
    margin: 0 0 10px 0;
}

.sample-note {
    display: inline-block;
    background: #fff3e0;
    color: #f57c00;
    padding: 4px 10px;
    border-radius: 12px;
    font-size: 12px;
    font-weight: var(--font-weight-medium);
}

/* International Experience */
.experience-stats {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
    gap: 20px;
    margin-bottom: 30px;
}

.stat-card {
    display: flex;
    align-items: center;
    gap: 15px;
    padding: 20px;
    background: var(--bg-white);
    border-radius: 15px;
    border: 2px solid #f1f2f6;
    transition: all 0.3s ease;
}

.stat-card:hover {
    border-color: #ff6b6b;
    transform: translateY(-3px);
    box-shadow: 0 10px 25px rgba(255, 107, 107, 0.15);
}

.stat-icon {
    width: 50px;
    height: 50px;
    background: linear-gradient(135deg, #ff9800, #ffb74d);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--bg-white);
    font-size: 20px;
    flex-shrink: 0;
}

.stat-content {
    text-align: left;
}

.stat-num {
    font-size: 1.8rem;
    font-weight: var(--font-weight-bold);
    color: #ff6b6b;
    display: block;
    line-height: 1;
}

.stat-desc {
    font-size: 14px;
    color: #7f8c8d;
    font-weight: var(--font-weight-medium);
}

.key-markets h4 {
    font-size: 18px;
    font-weight: var(--font-weight-semibold);
    color: #2c3e50;
    margin: 0 0 15px 0;
}

.market-tags {
    display: flex;
    flex-wrap: wrap;
    gap: 10px;
}

.market-tag {
    background: var(--bg-white);
    border: 2px solid #f1f2f6;
    padding: 8px 15px;
    border-radius: 20px;
    font-size: 14px;
    font-weight: var(--font-weight-medium);
    color: #2c3e50;
    transition: all 0.3s ease;
}

.market-tag:hover {
    border-color: #ff6b6b;
    background: #ff6b6b;
    color: var(--bg-white);
    transform: translateY(-2px);
}

/* FAQ Footer */
.faq-footer {
    margin-top: 30px;
    padding: 25px;
    background: linear-gradient(135deg, #2c3e50, #3498db);
    border-radius: 20px;
    color: var(--bg-white);
}

.footer-content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    gap: 40px;
    margin-bottom: 30px;
}

.contact-prompt {
    display: flex;
    align-items: center;
    gap: 20px;
    flex: 1;
}

.prompt-icon {
    width: 60px;
    height: 60px;
    background: rgba(255, 255, 255, 0.2);
    border-radius: 15px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 24px;
    flex-shrink: 0;
}

.prompt-text h3 {
    font-size: var(--font-size-2xl);
    font-weight: var(--font-weight-semibold);
    margin: 0 0 5px 0;
    color: var(--bg-white) !important;
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.prompt-text p {
    font-size: 16px;
    opacity: 1;
    margin: 0;
    color: rgba(255, 255, 255, 0.95) !important;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
}

.contact-actions {
    display: flex;
    gap: 15px;
    flex-wrap: wrap;
}

.contact-actions .btn {
    padding: 12px 24px;
    border-radius: 25px;
    font-weight: var(--font-weight-semibold);
    text-decoration: none;
    display: flex;
    align-items: center;
    gap: 8px;
    transition: all 0.3s ease;
}

.contact-actions .btn-primary {
    background: #ff6b6b;
    color: var(--bg-white);
    border: 2px solid #ff6b6b;
}

.contact-actions .btn-secondary {
    background: #27ae60;
    color: var(--bg-white);
    border: 2px solid #27ae60;
}

.contact-actions .btn-outline {
    background: transparent;
    color: var(--bg-white);
    border: 2px solid rgba(255, 255, 255, 0.3);
}

.contact-actions .btn:hover {
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);
}

.quick-contact-info {
    display: flex;
    justify-content: space-between;
    align-items: center;
    gap: 30px;
    padding-top: 30px;
    border-top: 1px solid rgba(255, 255, 255, 0.2);
}

.quick-contact-info .contact-item {
    display: flex;
    align-items: center;
    gap: 10px;
    font-size: 14px;
    opacity: 0.9;
}

.quick-contact-info .contact-item i {
    font-size: 16px;
}

/* Responsive Design */
@media (max-width: 1024px) {
    .faq-header {
        flex-direction: column;
        text-align: center;
        gap: 30px;
    }

    .faq-stats {
        justify-content: center;
    }

    .faq-main-title {
        font-size: 2.4rem;
